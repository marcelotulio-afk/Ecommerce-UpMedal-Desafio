{
  "store.home": {
    "blocks": [
      "tab-layout",
      "slider-layout#3",
      "slider-layout#2",
      "responsive-layout.desktop#desktop",
      "responsive-layout.mobile#mobile",
      "rich-text#novo",
      "list-context.product-list",
      "search-result-layout.customQuery#home"
    ]
  },
// tab-layout - menu de abas
    "tab-layout": {
      "children": ["tab-list"]
    },
    "tab-list": {
      "children": ["tab-list.item#1", "tab-list.item#2", "tab-list.item#3", "tab-list.item#4" ]
    },
    "tab-list.item#1": {
    "props": {
      "tabId": "promocao",
      "label": "Promoção",
      "defaultActiveTab": true
      }
    },
    "tab-list.item#2": {
      "props": {
        "tabId": "corinthians",
        "label": "Corinthians"
      }
    },
    "tab-list.item#3": {
      "props": {
        "tabId": "palmeiras",
        "label": "Palmeiras"
      }
    },
    "tab-list.item#4": {
      "props": {
        "tabId": "divertidos",
        "label": "Divertidos"
      }
    },
      "responsive-layout.desktop#desktop": {
      "children": ["rich-text#desktop" ]
    },
    "responsive-layout.mobile#mobile": {
      "children": ["rich-text#mobile"]
    },
// slider-layout#3 - 2 banners principais  
   "slider-layout#3": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "autoplay": { "timeout": 7000 },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    },
    "children": ["image#cta1", "image#cta2"]
  },
  "image#cta1": {
    "props": {
        "blockClass": "cover",
     "width": "100%",
      "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/6iibxhv7jzzdn2749uhoxub2fdjf?response-content-disposition=inline%3B%20filename%3D%22BANNER-WOOD.png%22%3B%20filename%2A%3DUTF-8%27%27BANNER-WOOD.png&response-content-type=image%2Fpng&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Credential=AKIAIRHGNFPZAGTZ7SUA%2F20210819%2Fsa-east-1%2Fs3%2Faws4_request&X-Amz-Date=20210819T222959Z&X-Amz-Expires=300&X-Amz-SignedHeaders=host&X-Amz-Signature=0c9d2cc58c4890cb332a001e476ef35d001e67f88dbf6f294002a9c2cb8e5a4b"
    }
  },
  "image#cta2": {
    "props": {
     "blockClass": "cover",
     "width": "100%",
      "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/du3k6akoks0u3xe322aypnaaal8e?response-content-disposition=inline%3B%20filename%3D%22W6S9NXIJ40I86DJI17S40123EYNM3J6CNF24M4Y04GGC9TBSXU.png%22%3B%20filename%2A%3DUTF-8%27%27W6S9NXIJ40I86DJI17S40123EYNM3J6CNF24M4Y04GGC9TBSXU.png&response-content-type=image%2Fpng&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Credential=AKIAIRHGNFPZAGTZ7SUA%2F20210819%2Fsa-east-1%2Fs3%2Faws4_request&X-Amz-Date=20210819T222959Z&X-Amz-Expires=300&X-Amz-SignedHeaders=host&X-Amz-Signature=e330f9ca8163bdee45d3db539f0b7aa53e97b784ae1b20cc4d1e9e2648025960"
    }
  },
    "slider-layout#teste": {
      "props": {
        "itemsPerPage": 8
        }
    },
// slider layout#2
  "slider-layout#2": {
    "children": ["flex-layout.row#1", "flex-layout.row#2"],
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "autoplay": {
        "timeout": 6000,
        "stopOnHover": false
      }
    }
  },
// flex-layout#1
  "flex-layout.row#1": {
    "children": ["info-card#produto-destaque1", "flex-layout.col#1"]
  },
  "flex-layout.col#1": {
    "children": [
      "info-card#produto1",
      "info-card#produto2"
    ]
  },
  "info-card#produto-destaque1": {
