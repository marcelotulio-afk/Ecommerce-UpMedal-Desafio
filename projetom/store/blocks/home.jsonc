      "headline": "Etapa Aeronáutica",
//   "subhead": "Coleção Forças Armadas",
      "callToActionMode": "button",
      "callToActionText": "Ver Detalhes",
      "callToActionUrl": "/sale/d",
      "textAlignment": "center"
    }
  },
  "info-card#produto2": {
    "props": {
      "isFullModeStyle": false,
      "textPosition": "right",
      "imageUrl": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/1627507672-POST%20MEDALHA.png",
      "headline": "Wood Sport",
//   "subhead": "Festival",
      "callToActionMode": "button",
      "callToActionText": "Ver Detalhes",
      "callToActionUrl": "/sale/d2",
      "textAlignment": "center"
    }
  },
// flex-layout#2
  "flex-layout.row#2": {
    "children": ["info-card#produto-destaque2", "flex-layout.col#2"]
  },
  "flex-layout.col#2": {
    "children": [
      "info-card#produto3",
      "info-card#produto4"
    ]
  },
  "info-card#produto-destaque2": {
    "props": {
      "isFullModeStyle": true,
      "imageUrl": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/1624896612-AGORA%20CORRE.png",
      "headline": "Desafio Agora Corre",
      "callToActionText": "Ver Detalhes",
      "textPosition": "right"
    }
  },
  "info-card#produto3": {
    "props": {
      "isFullModeStyle": false,
      "textPosition": "right",
      "imageUrl": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/1628715231-POST.png",
      "headline": "King",
//   "subhead": "Challenge",
      "callToActionMode": "button",
      "callToActionText": "Ver Detalhes",
      "callToActionUrl": "/sale/d",
      "textAlignment": "center"
    }
  },
  "info-card#produto4": {
    "props": {
      "isFullModeStyle": false,
      "textPosition": "right",
      "imageUrl": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/1623790356-xtreme-post22.png",
      "headline": "Extreme",
//  "subhead": "Virtual Games",
      "callToActionMode": "button",
      "callToActionText": "Ver Detalhes",
      "callToActionUrl": "/sale/d2",
      "textAlignment": "center"
    }
  },

// responsive layout para desktop e para mobile
  "rich-text#desktop": {
    "props": {
      "text": "### Inovação de alta performance",
      "textPosition": "CENTER",
      "textAlignment": "CENTER"
      // ### Your Coffee, Your Way \n
    }
  },

  "rich-text#mobile": {
    "props": {
      "text": " ### Inovação de alta performance",
      "textPosition": "CENTER",
      "textAlignment": "CENTER"
      // #### Your Coffee, Your Way \n
    }
  },
  "rich-text#novo": {
    "props": {
      "text": "### *Conheça a coleção inteira abaixo...*",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-heading-1"
    }
  },
// search-result - 8 items por página
  "search-result-layout.customQuery#home": {
    "props": {
      "querySchema": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "skip",
        "maxItemsPerPage": 8,
        "queryField": "Kit UpMedal"
      }
  },
  "blocks":["search-result-layout.desktop#lp", "search-result-layout.mobile"]
  },
  "search-result-layout.desktop#lp":{
    "children": [

      "search-content#lp"
    ],
    "props": {
      "preventRouteChange": true
    }
  },
  "search-content#lp": {
    "blocks": [
      "gallery#lp", "not-found"
    ]
  },
// product-summary --> informações sobre os produtos nas prateleiras

  "product-summary.shelf#1": {
    "children": [
      "product-summary-name"
    ]
  },
  "gallery#lp": {
    "props": {
      "layouts": [
        {
          "name": "grid",
          "component": "GridSummary",
          "itemsPerRow": {
            "(min-width:1300px)": 4,
            "desktop": 3,
            "tablet": 3,
            "phone": 2
          }
        },
        {
          "name": "list",
          "component": "ListSummary",
          "itemsPerRow": 1
        }
      ],
      "ListSummary": "product-summary.shelf#lp",
      "GridSummary": "product-summary.shelf#lp"
    }
  },
  "product-summary.shelf#lp": {
    "children": [
      "product-summary-name",
      "product-summary-image",
      "vtex.product-highlights@2.x:product-highlights"
/*       "product-summary-description",
      "product-summary-price",
      "product-summary-sku-selector",
      "product-summary-buy-button" */
    ]
  },
  "product-images#lp": {
    "props": {
      "showNavigationArrows": true,
      "showPaginationDots": true,
      "thumbnailMaxHeight": 1
    }
  },
  "vtex.product-highlights@2.x:product-highlights": {
    "children": ["product-highlight-text"]
  },
  "product-highlight-text": {
    "props": {
      "message": "me salva"
    }
  }
}
